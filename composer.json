{
    "name": "joedaco/quickbooks-api-bundle",
    "description": "A REST API Integration Module for QuickBooks Online and Symfony 4.",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Joe Daigle",
            "email": "josephldaigle@yahoo.com"
        }
    ],
    "require": {
        "symfony/config": "^4",
        "symfony/framework-bundle": "^4",
        "symfony/dependency-injection": "^4",
        "symfony/http-kernel": "^4",
        "symfony/http-client": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "JoeDaCo\\QuickBooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoeDaCo\\QuickBooks\\Test\\": "Tests/"
        }
    }
}
